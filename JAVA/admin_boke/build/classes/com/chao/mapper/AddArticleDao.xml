<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
		
<mapper namespace="com.chao.mapper.AddArticleDao">
	<insert id="addArticle" parameterType="Article">
		insert into article(title,author,content,img,articleTime,pageView,critiques,articleType)
		values(#{title},#{author},#{content},#{img},#{articleTime},#{pageView},#{critiques},#{articleType})
	</insert>
</mapper>